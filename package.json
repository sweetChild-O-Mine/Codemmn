{
  "name": "codemmn",
  "publisher": "Slashhh",
  "displayName": "Codemmn",
  "description": "generates notes by analyzing you code and comments",
  "repository": {
    "type": "git",
    "url": "https://github.com/sweetChild-O-Mine/Codemmn.git"
  },
  "icon": "icon.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Codemmn",
      "properties": {
        "codemmn.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Enter your Gemini API Key here. Yahan dalo!"
        }
      }
    },
    "commands": [
      {
        "command": "codemmn.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "dotenv": "^17.2.3"
  }
}
